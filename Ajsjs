-- Myleedeadrails Script -- New example script written by wally, customized for Dead Rails

local repo = 'https://raw.githubusercontent.com/LionTheGreatRealFrFr/MobileLinoriaLib/main/' local Library = loadstring(game:HttpGet(repo .. 'Library.lua'))() local ThemeManager = loadstring(game:HttpGet(repo .. 'addons/ThemeManager.lua'))() local SaveManager = loadstring(game:HttpGet(repo .. 'addons/SaveManager.lua'))()

-- Create Window local Window = Library:CreateWindow({ Title = 'Myleedeadrails', Center = true, AutoShow = true, TabPadding = 8, MenuFadeTime = 0.2 })

-- Tabs local Tabs = { Main = Window:AddTab('Main'), ['UI Settings'] = Window:AddTab('UI Settings'), }

-- Main Groupbox local MainGroup = Tabs.Main:AddLeftGroupbox('Features')

-- Kill Aura Toggle MainGroup:AddToggle('KillAura', { Text = 'Kill Aura', Default = false, Tooltip = 'Automatically attack nearby players', Callback = function(val) getgenv().KillAuraEnabled = val end })

-- Auto Collect Bonds Toggle MainGroup:AddToggle('AutoCollect', { Text = 'Auto Collect Bonds', Default = false, Tooltip = 'Collect all bonds automatically', Callback = function(val) getgenv().

